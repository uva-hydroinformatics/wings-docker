
<process proc_name="Evaporation" method_name="Priestley-Taylor">
<proc_name> Evaporation </proc_name>
<method_name> Priestley-Taylor </method_name>

<layer>
    <variable>
        <name> net shortwave radiation </name>
        <label> Qnet_SW </label>
        <units> W/m**2 </units>
        <value> 100.0 </value>
        <type> Scalar </type>
        <type_choices> Scalar, Time series, Grid, Grid sequence </type_choices>
    </variable>

    <variable>
        <name> net longwave radiation </name>
        <label> Qnet_LW </label>
        <units> W/m**2 </units>
        <value> 10.0 </value>
        <type> Scalar </type>
        <type_choices> Scalar, Time series, Grid, Grid sequence </type_choices>
    </variable>

    <variable>
        <name> air temperature </name>
        <label> T_air </label>
        <units> deg_C </units>
        <value> 20.0 </value>
        <type> Scalar </type>
        <type_choices> Scalar, Time series, Grid, Grid sequence </type_choices>
    </variable>

    <variable>
        <name> surface temperature </name>
        <label> T_surf </label>
        <units> deg_C </units>
        <value> -5.0 </value>
        <type> Scalar </type>
        <type_choices> Scalar, Time series, Grid, Grid sequence </type_choices>
    </variable>

    <variable>
        <name> soil temperature at depth x </name>
        <label> T_soil_x </label>
        <units> deg_C </units>
        <value> 0.0 </value>
        <type> Scalar </type>
        <type_choices> Scalar, Time series, Grid, Grid sequence </type_choices>
    </variable>

    <variable>
        <name> reference depth </name>
        <label> x </label>
        <units> cm </units>
        <value> 5.0 </value>
        <type> Scalar </type>
        <type_choices> Scalar, Time series, Grid, Grid sequence </type_choices>
    </variable>

    <variable>
        <name> soil hydraulic conductivity </name>
        <label> K_soil </label>
        <units> W/m/deg_C </units>
        <value> 0.45 </value>
        <type> Scalar </type>
        <type_choices> Scalar, Time series, Grid, Grid sequence </type_choices>
    </variable>

    <variable>
        <name> P-T coefficient </name>
        <label> alpha </label>
        <units> none </units>
        <value> 1.2 </value>
        <type> Scalar </type>
        <type_choices> Scalar, Time series, Grid, Grid sequence </type_choices>
    </variable>
	
    <timestep>
        <label> ET process timestep </label>
        <value> 3600.0 </value>
        <units> seconds </units>
    </timestep>
	
    <help_file> /Applications/TopoFlow/help/ET_Priestley-Taylor.htm
    </help_file>

</layer>
</process>
